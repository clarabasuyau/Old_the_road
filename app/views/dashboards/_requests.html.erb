<div class="card-product">
  <%= cl_image_tag request.user.photo.key if request.user.photo.key %>
  <div class="card-product-infos">
    <%# <h5><%= request.start_rent %></h5>
    <%# <h5><%= request.end_rent %></h5>
    <%# <h5><%= request.message %></h5>
    <h5><%= request.status %></h5>
    <%#= car.overview %>
    <% if request.pending? %>
      <div class="notification-actions d-flex">
        <%=link_to "✏️",  car_booking_path(request.car, request, booking: {status: 1}), data: { turbo_method: :patch } %>
        <%=link_to "🗑", booking_path(request), data: {turbo_method: 'delete'} %>
      </div>
    <% end %>
  </div>
</div>
